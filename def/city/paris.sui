city_data: city.paris
{
	city_name: "Paris"
	city_name_localized: "@@paris@@"
	country: france

	map_x_offsets[]: 0
	map_x_offsets[]: 0
	map_x_offsets[]: 0
	map_x_offsets[]: -10
	map_x_offsets[]: -50
	map_x_offsets[]: -40
	map_x_offsets[]: -35
	map_x_offsets[]: -28

	map_y_offsets[]: 0
	map_y_offsets[]: 0
	map_y_offsets[]: 25
	map_y_offsets[]: -28
	map_y_offsets[]: -230
	map_y_offsets[]: -60
	map_y_offsets[]: -35
	map_y_offsets[]: -30

	### VERY LARGE CITY ###
	city_pin_scale_factor[]: 1.3
	city_pin_scale_factor[]: 1.3
	city_pin_scale_factor[]: 1.2  // close zoom for minimap
	city_pin_scale_factor[]: 1.2  // far zoom for minimap
	city_pin_scale_factor[]: 1.5  // city zoom for world map
	city_pin_scale_factor[]: 1.4  // closest zoom for world map
	city_pin_scale_factor[]: 1.3  // middle zoom for world map
	city_pin_scale_factor[]: 1.2  // whole world map zoom

	vehicle_brands[]:"renault"

	license_plate[]: .paris.lp.car
	license_plate[]: .paris.lp.truck
	license_plate[]: .paris.lp.trailer
}

