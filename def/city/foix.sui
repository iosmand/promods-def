	map_y_offsets[]: 0
	map_y_offsets[]: -5
	map_y_offsets[]: 40
	map_y_offsets[]: -10
	map_y_offsets[]: -8
	map_y_offsets[]: -8

	### SMALL CITY ###
	city_pin_scale_factor[]: 1.0
