{
	type: truck

@include "city_lp_fr_truck.sui"

	def5[]:"</align><ret><align right=273><offset vshift=-5><img src=/material/ui/lp/france/57.mat color=FFFFFFFF></align>"
}

license_plate_data : .metz.lp.trailer
{
	type: trailer

@include "city_lp_fr_trailer.sui"

