city_data: city.nice
{
	city_name: "Nice"
	city_name_localized: "@@nice@@"
	country: france

	map_x_offsets[]: 0
	map_x_offsets[]: 0
	map_x_offsets[]: 30
	map_x_offsets[]: -7
	map_x_offsets[]: 50
	map_x_offsets[]: -5
	map_x_offsets[]: -13
	map_x_offsets[]: -19

	map_y_offsets[]: 0
	map_y_offsets[]: 0
	map_y_offsets[]: -70
	map_y_offsets[]: -21
	map_y_offsets[]: -250
	map_y_offsets[]: -55
	map_y_offsets[]: -19
	map_y_offsets[]: -22

	### MEDIUM CITY ###
	city_pin_scale_factor[]: 1.0
	city_pin_scale_factor[]: 1.0
	city_pin_scale_factor[]: 0.9  // close zoom for minimap
	city_pin_scale_factor[]: 0.8  // far zoom for minimap
	city_pin_scale_factor[]: 1.0  // city zoom for world map
	city_pin_scale_factor[]: 1.0  // closest zoom for world map
	city_pin_scale_factor[]: 0.9  // middle zoom for world map
	city_pin_scale_factor[]: 0.8  // whole world map zoom

	license_plate[]: .nice.lp.car
	license_plate[]: .nice.lp.truck
	license_plate[]: .nice.lp.trailer
}

license_plate_data : .nice.lp.car
{
	type: car

@include "city_lp_fr_car.sui"

	def5[]:"</align><ret><align right=273><offset vshift=-5><img src=/material/ui/lp/france/06.mat color=FFFFFFFF></align>"
}

license_plate_data : .nice.lp.truck
{
	type: truck

@include "city_lp_fr_truck.sui"

	def5[]:"</align><ret><align right=273><offset vshift=-5><img src=/material/ui/lp/france/06.mat color=FFFFFFFF></align>"
}

license_plate_data : .nice.lp.trailer
{
	type: trailer

@include "city_lp_fr_trailer.sui"

	def5[]:"</align><ret><align right=273><offset vshift=-5><img src=/material/ui/lp/france/06.mat color=FFFFFFFF></align>"
}
