traffic_vehicle : traffic.incentro
{
	accessories[]: .incentro.chassis
	accessories[]: .incentro.fwheel
	accessories[]: .incentro.rwheel

	max_speed: 50

	vehicle_mass: 100000
	engine_power: 360	# 8x45 kW
	spawn_ratio: 0	

	sound_move[]: "/sound/traffic/tram_1.soundref"
	sound_horn[]: "/sound/traffic/tram_horn_1.soundref"
	
	trailer_chains[]: "train_car.incentro.first | train_car.incentro.second"

	displacement_limit: 0.0

	lod_dist[]: 70
	# add lod in between
	lod_dist[]: 130
	lod_dist[]: 200

	prefer_variant: true
	variant[]: traffic.incentro.brest
	variant[]: traffic.incentro.clermont
	variant[]: traffic.incentro.nantes
	variant[]: traffic.incentro.toulouse
	variant[]: traffic.incentro.orleans
	variant[]: traffic.incentro.poznan
}
vehicle_accessory: .incentro.chassis
{
	data_path: "/def/vehicle/ai/incentro/chassis_locomotive.sii"
}
vehicle_wheel_accessory: .incentro.fwheel
{
	data_path: "/def/vehicle/ai_wheel/incentro_front.sii"
}
vehicle_wheel_accessory: .incentro.rwheel
{
	data_path: "/def/vehicle/ai_wheel/incentro_rear.sii"
}



